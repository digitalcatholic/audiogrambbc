{
  "name": "audiogram",
  "version": "3.6.1",
  "description": "An audio to video and subtitling tool",
  "main": "index.js",
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "start": "bin/server",
    "debug": "DEBUG=1 bin/server",
    "worker": "bin/worker",
    "build": "mkdir -p editor/js && browserify client/index.js > editor/js/bundle.js",
    "watch": "mkdir -p editor/js && watchify client/index.js -o editor/js/bundle.js",
    "test": "rm -rf test/tmp && tape 'test/**/*-test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nypublicradio/audiogram.git"
  },
  "keywords": [],
  "author": {
    "name": "Jonty Usborne",
    "url": "https://twitter.com/JontyUsborne"
  },
  "contributors": [
    {
      "name": "Noah Veltman",
      "url": "https://twitter.com/veltman"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "aws-sdk": "2.9.0",
    "body-parser": "^1.17.1",
    "browserify": "13.0.0",
    "browserify-versionify": "^1.0.6",
    "canvas": "git+https://github.com/chearon/node-canvas.git#12971f64a66b",
    "compression": "1.6.2",
    "concat-files": "^0.1.1",
    "config": "^1.28.1",
    "d3": "4.5.0",
    "dateformat": "^2.0.0",
    "delete-empty": "^1.0.1",
    "dotenv": "2.0.0",
    "express": "4.14.1",
    "fs-extra": "^5.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "jquery": "2.2.4",
    "lodash": "^4.17.4",
    "mkdirp": "0.5.1",
    "morgan": "1.8.0",
    "multer": "1.3.0",
    "node-ffprobe": "1.2.2",
    "node-gyp": "^3.8.0",
    "node-statsd": "^0.1.1",
    "node-uuid": "1.4.7",
    "os": "^0.1.1",
    "rebuild": "^0.1.2",
    "redis": "^2.8.0",
    "request": "^2.81.0",
    "rimraf": "2.5.4",
    "smartquotes": "1.0.0",
    "underscore": "1.8.3",
    "uuid": "^3.0.1",
    "webaudio-peaks": "0.0.5",
    "winston": "2.3.1",
    "xml2json": "^0.11.0"
  },
  "devDependencies": {
    "supertest": "1.2.0",
    "tape": "4.6.0",
    "watchify": "3.7.0"
  },
  "browserify": {
    "transform": [
      "browserify-versionify"
    ]
  }
}
